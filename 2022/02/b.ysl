var score = 0


main:
    getch
    var i f return
    lt $i 0
    goto_if *exit
    getch
    getch
    var j f return
    var i + $j
    var i - 1
    var i % 3
    var score + $i
    var score + 1
    var j - 1
    var j % 3
    var j * 3
    var score + $j
    getch
    var i f return
    cmp 10 $i
    goto *main
exit:
    println $score
    exit
